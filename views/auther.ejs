<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
      rel="stylesheet"
    />
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/main.css" />
    <title>Portfolio</title>
  </head>
  <body>
    <div class="frame">
      <div class="main-frame">
        <div class="left-frame">
          <div class="left-1">
            <div class="left-1-1">
              <img
                src="images/profile.png"
                width="156px"
                height="142px"
                alt=""
              />
            </div>
            <div class="left-1-2"><%=user.name%></div>
            <div class="left-1-3"><%=user.profession%></div>
            <div class="left-icons">
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-facebook.svg" alt="" class="icon-1" />
                </div>
              </a>
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-instagram.svg" alt="" class="icon-1" />
                </div>
              </a>
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-dribbble.svg" alt="" class="icon-1" />
                </div>
              </a>
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-linkedin.svg" alt="" class="icon-1" />
                </div>
              </a>
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-twitter.svg" alt="" class="icon-1" />
                </div>
              </a>
              <a href="" class="link-href">
                <div class="icon">
                  <img src="icon/Icons-Youtube.svg" alt="" class="icon-1" />
                </div>
              </a>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left-2">
            <div class="left-2-1">
              <div class="left-p">Age:</div>
              <p><%=user.age%></p>
            </div>
            <div class="left-2-2">
              <div class="left-p">Residence:</div>
              <p><%=user.residence%></p>
            </div>
            <div class="left-2-3">
              <div class="left-p">Freelance:</div>
              <p><%=user.freelance%></p>
            </div>
            <div class="left-2-4">
              <div class="left-p">Adress:</div>
              <p><%=user.adress%></p>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left-3">
            <div class="left-3-1">
              <h3>Languages</h3>
            </div>
            <div class="left-3-2">
              <p><%=user.languages%></p>
              <p>100%</p>
            </div>
            <div class="yellow-line">
                <div class="fill-1"></div>
            </div>
            <div class="left-3-3">
              <p><%=user.languages1%></p>
              <p>100%</p>
            </div>
            <div class="yellow-line">
                <div class="fill-1"></div>
            </div>
            <div class="left-3-4">
              <p><%=user.languages2%></p>
              <p>50%</p>
            </div>
            <div class="yellow-line">
              <div class="fill"></div>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left-4">
  <h3>Skills</h3>

  <div class="skill">
    <div class="skill-text">
      <p><%=user.skills%></p>
      <p>90%</p>
    </div>
    <div class="yellow-line">
      <div class="fill" style="width: 90%"></div>
    </div>
  </div>

  <div class="skill">
    <div class="skill-text">
      <p><%=user.skills%></p>
      <p>85%</p>
    </div>
    <div class="yellow-line">
      <div class="fill" style="width: 85%"></div>
    </div>
  </div>

  <div class="skill">
    <div class="skill-text">
      <p><%=user.skills%></p>
      <p>80%</p>
    </div>
    <div class="yellow-line">
      <div class="fill" style="width: 80%"></div>
    </div>
  </div>

  <div class="skill">
    <div class="skill-text">
      <p><%=user.skills%></p>
      <p>75%</p>
    </div>
    <div class="yellow-line">
      <div class="fill" style="width: 75%"></div>
    </div>
  </div>

  <div class="skill">
    <div class="skill-text">
      <p><%=user.skills%></p>
      <p>85%</p>
    </div>
    <div class="yellow-line">
      <div class="fill" style="width: 85%"></div>
    </div>
  </div>
</div>

          <div class="marginer"></div>
          <div class="left-5"></div>
          <div class="marginer"></div>
        </div>
        <div class="midd-frame"></div>
        <div class="right-frame"></div>
      </div>
    </div>
  </body>
</html>
